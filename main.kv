                hint_text_color_focus: "000000"
                line_color_normal: "000000"
                line_color_focus: "000000"
                hint_text: "Password"
                helper_text: ""
                helper_text_color_normal: "000000"
                helper_text_color_focus: "000000"
                icon_left_color_normal: "000000"
                icon_left_color_focus: "000000"
                password: True
                icon_left: "lock-open"
            MDFlatButton:
                size_hint_x: .5
                pos_hint: {"center_x":.78}
                text: "Forgot Password?"
                font_style: "Overline"
                on_press:
                    root.manager.transition.direction = 'left'
                    app.root.current = "otpverify"
            MDGridLayout:
                cols:3
                spacing: 10
            MDRoundFlatButton:
                text: "Login"
                font_style: "Button"
                pos_hint: {"center_x":.5, "center_y":.35}
                md_bg_color: 0, .1, .5, 1
                on_press:    
                    
