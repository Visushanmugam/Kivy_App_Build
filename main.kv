<Imagestart>:
    name: "imagestart"
    MDScreen:
        Image:
            source: "Logo_Final.png"
            size_hint_y: .9
            pos_hint: {"top": 1}
            radius: 3, 3, 0, 0
        RelativeLayout:
            size_hint_y: .4
            MDProgressBar:
                id: spinner
                type: "determinate"
                size_hint_x:.5
                size_hint_y: None
                height: dp(5)
                pos_hint:{"center_x": .5, 'center_y': .05}
                on_kv_post:
                    self.start()
<Login>:
    name: "login"
    MDScreen:
        md_bg_color: "ffffff"
        MDBoxLayout:
            orientation: "vertical"
            spacing: 50
            padding: 20, 20
            MDLabel:
                text: "Login"
                halign: "center"
                bold: True
                font_style:"H4"
            MDTextField:
                id: phone_login
                pos_hint: {"center_x":.5, "center_y":.65}
                hint_text: "Phone Number"
                text_color_normal: "000000"
                text_color_focus: "000000"
                hint_text_color_normal: "000000"
                hint_text_color_focus: "000000"
                size_hint_x: .7
                line_color_normal: "000000"
                line_color_focus: "000000"
                multiline: False
                max_text_length: "10"
                helper_text_color_normal: "000000"
                helper_text_color_focus: "000000"
                icon_left: "account"
                icon_left_color_normal: "000000"
                icon_left_color_focus: "000000"
